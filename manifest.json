{
  "name": "A11Y Chatbot",
  "version": "1.0.0",
  "description": "A11Y Chatbot teaches you how to build Websites and Apps accessible to all.",
  "manifest_version": 3,
  "author": "A11Y Chatbot Team",
  "action": {
    "default_popup":"script.js",
    "default_icon": "defaultLogo.png" 
  },
  "icons": {
    "128": "defaultLogo.png"
},
  "permissions": [
      "activeTab",
      "scripting",
      "webNavigation",
      "storage"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "css": ["styles.css"]
      }
    ],
    "web_accessible_resources": [ {
      "matches": [ "<all_urls>" ],
      "resources": [ "styles.css","defaultLogo.png","logo.png","send.svg"]
   } ]
    
}