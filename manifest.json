  {
    "manifest_version": 3,
    "name": "A11Y Chatbot",
    "version": "1.0",
    "description": "A11Y Chatbot teaches you how to build Websites and Apps accessible to all.",
    "action": {
      "default_icon": "chatbot.png" 
    },
    "icons": {
      "128": "chatbot.png"
  },
    "permissions": [
      "activeTab",
      "scripting",
      "webNavigation",
      "storage"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"],
        "css": ["style.css"]
      }
    ],
    "web_accessible_resources": [ {
      "matches": [ "<all_urls>" ],
      "resources": ["style.css", "logo.png", "send.svg"]
   } ]
}  